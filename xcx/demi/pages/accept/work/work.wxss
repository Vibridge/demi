.loading{
  width: 100rpx;
  height: 100rpx;
  margin-top: 80%
}
.contain {
  margin: 129rpx auto 0 auto;
}


.container {
  padding: 30rpx 25rpx 0 0;
}

.main {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.first_label {
  padding: 18rpx 58rpx;
  color: #333;
  font-size: 32rpx;
  width: 190rpx;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
}

.second_label {
  width: 550rpx;
  font-size: 26rpx;
}

.label {
  display: inline-block;
  width: 155rpx;
  height: 66rpx;
  line-height: 64rpx;
  border: 1rpx solid rgba(204, 204, 204, 1);
  border-radius: 8rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  text-align: center;
}

.active_label {
  color: white;
  background: #24bfff;
  border: none !important;
}
