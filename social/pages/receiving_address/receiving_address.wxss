.wrap{
  height: 100%;
  background:rgba(249,249,249,1);
}

.address_main{
  background: #ffffff;
}

.address_info{
  display: flex;
  padding: 17rpx 0 15rpx;
  margin: 0 33rpx 0 30rpx;
  justify-content: space-between;
  border-bottom: 1rpx solid #E4E4E4;
}

.name,.phone{
  font-size: 32rpx;
  color: #333333;
  margin-right: 25rpx;
}

.address{
  color:#CCCCCC;
  font-size: 28rpx;
  margin-top: 9rpx;
}

navigator{
  display: flex;
}

.address_info image{
  width: 32rpx;
  align-self: center;
}

/* 底部 */
.bottom{
  left: 0;
  bottom: 0;
  width: 100%;
  display: flex;
  height: 98rpx;
  position: fixed;
  background: #fff;
  text-align: center;
}

.coc{
  height:72rpx;
  width:690rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #333333;
  align-self: center;
  line-height: 72rpx;
  border-radius:36rpx;
  background:linear-gradient(46deg,rgba(255,226,0,1) 0%,rgba(255,187,0,1) 100%);
}