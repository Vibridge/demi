.form{
  padding: 0 32rpx;
}

.label,.default{
  display: flex;
  padding: 30rpx 0;
  justify-content: space-between;
  border-bottom: 1rpx solid #E4E4E4;
}

.title{
  color:#666666;
  font-size: 28rpx;
  align-self: center;
}

.edit{
  display: flex;
  align-self: center;
}

icon{
  align-self: center;
}

.space{
  width: 40rpx;
  height: 40rpx;
  align-self: center;
}

input{
  width: 440rpx;
  align-self: center;
  margin-right: 49rpx;
}

switch{
  width: 102rpx;
  height: 54rpx;
}
/* 底部 */
.bottom{
  left: 0;
  bottom: 0;
  width: 100%;
  display: flex;
  height: 98rpx;
  position: fixed;
  background: #fff;
  text-align: center;
}

.coc{
  height:72rpx;
  width:690rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #333333;
  align-self: center;
  line-height: 72rpx;
  border-radius:36rpx;
  background:linear-gradient(46deg,rgba(255,226,0,1) 0%,rgba(255,187,0,1) 100%);
}
